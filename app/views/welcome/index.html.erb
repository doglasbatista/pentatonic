<% cache ['store', Product.last] do %>
  <% @products.each do |product| %>
    <% cache ['entry', product] do %>
      <div class="entry">
        <div class="row">
          <div class="col-xs-6 col-md-3">             
            <a class="thumbnail" style="width: 200px  " <%= link_to (image_tag product.cover.url(:small)), product %>     
            </a>
          </div>
        </div>
        <%= product.title %>
      </div>      
    <% end %>
  <% end %>
<% end %>
